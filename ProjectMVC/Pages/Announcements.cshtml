@page
@model ProjectMVC.Pages.AnnouncementsModel
@{
}

<h1> All Anouncements:</h1>
<table class="table table-hover">
    <thead>
        <tr>
            <td>
                Time:
            </td>
            <td>
                Message:
            </td>
        </tr>
    </thead>
    <tbody>
@for (int i = Model.Messages.Count() - 1; i >= 0; i--)
{
    <tr>
        <td>
        @Html.DisplayFor(modelitem => Model.Messages.ElementAt(i).timestamp)
            </td>

        <td>
        @Html.DisplayFor(modelitem => Model.Messages.ElementAt(i).mesage)
            </td>
        </tr>
}
    </tbody>
    </table>
<br />