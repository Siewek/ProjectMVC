@page
@model ProjectMVC.Pages.AnnouncementsModel
@{
}

<h1> All Anouncements:</h1>
@for (int i = Model.Messages.Count() - 1; i >= 0; i--)
{
    <div>
        <h4>@Html.DisplayFor(modelitem => Model.Messages.ElementAt(i).timestamp)</h4>
    </div>
    <div>
        <h4>@Html.DisplayFor(modelitem => Model.Messages.ElementAt(i).mesage)</h4>
    </div>
    <br />
}
<br />